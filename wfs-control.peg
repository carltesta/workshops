##JSoLang wfs-control

main = x:allRules* { return "##miniTidal\n" + x.join("") }
allRules = wfs / lag / n / x / y / anyCharacter
anyCharacter = .

wfs = "wfs"i { return  "pS \"s\" \"wfs\"" }
lag = "lag"i { return "|+| pF \"lag\"" }
n = "n"i { return "|+| pN \"n\"" }
x = "x"i {return "|+| pF \"x\"" }
y = "y"i { return "|+| pF \"y\"" }
